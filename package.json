{
  "type": "module",
  "name": "fastify-starter",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "version": "2024.8.4",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "vitest --run",
    "start": "npm run build && node dist/index.js",
    "build": "tsc && tsc-alias",
    "typecheck": "tsc --noEmit",
    "dev": "cross-env FASTIFY_AUTOLOAD_TYPESCRIPT=1 node --import=tsx --watch --env-file=.env ./src/index.ts",
    "check": "biome check --apply ./src",
    "check:unsafe": "biome check --apply-unsafe ./src",
    "release": "release-it"
  },
  "keywords": [],
  "author": "Douglas Moura <douglas.ademoura@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.8.1",
    "@fastify/sensible": "^5.6.0",
    "@fastify/swagger": "^8.14.0",
    "@scalar/fastify-api-reference": "^1.22.20",
    "close-with-grace": "^1.3.0",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.1",
    "fastify-type-provider-zod": "^2.0.0",
    "pino": "^9.3.2",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@csmith/release-it-calver-plugin": "^2023.12.0",
    "@types/node": "^20.12.8",
    "cross-env": "^7.0.3",
    "fastify-tsconfig": "^2.0.0",
    "pino-pretty": "^11.2.2",
    "release-it": "^17.6.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.17.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
